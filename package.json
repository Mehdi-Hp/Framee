	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --hide-modules --optimize-minimize --progress --config webpack.prod.config.js",
		"build:dev": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --config webpack.dev.config.js",
		"postinstall": "cross-env NODE_ENV=production webpack --hide-modules --optimize-minimize --progress --config webpack.prod.config.js",
		"start": "cross-env NODE_ENV=production DEBUG=app:* node ./bin/www",
		"start:dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon --delay 1 ./bin/www"
	},
	"nodemonConfig": {
		"ignore": ["node_modules"]
	},
	"now": {
		"dotenv": "./.env.now"
	},
	"hyperlayout": {
		"default": {
			"entry": "tab",
			"layout": ["mongod", "npm run start:dev", "npm run build:dev"]
		}
	},
